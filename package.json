{
    "name": "trackup-desktop-client",
    "version": "1.0.0",
    "description": "Desktop client for TrackUp",
    "main": "./out/main/index.js",
    "author": "example.com",
    "homepage": "https://electron-vite.org",
    "scripts": {
        "format": "prettier --write .",
        "lint": "eslint --cache .",
        "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
        "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
        "typecheck": "npm run typecheck:node && npm run typecheck:web",
        "start": "electron-vite preview",
        "dev": "electron-vite dev",
        "build": "npm run typecheck && electron-vite build",
        "postinstall": "electron-builder install-app-deps",
        "build:unpack": "npm run build && electron-builder --dir",
        "build:win": "npm run build && electron-builder --win",
        "build:mac": "npm run build && electron-builder --mac",
        "build:linux": "npm run build && electron-builder --linux && cp install-trackup.sh ./dist && chmod +x ./dist/install-trackup.sh"
    },
    "prettier": {
        "singleQuote": true,
        "semi": false,
        "printWidth": 100,
        "trailingComma": "none",
        "endOfLine": "lf",
        "tabWidth": 4
    },
    "dependencies": {
        "@electron-toolkit/preload": "^3.0.2",
        "@electron-toolkit/utils": "^4.0.0",
        "@fortawesome/fontawesome-svg-core": "^7.1.0",
        "@fortawesome/free-brands-svg-icons": "^7.1.0",
        "@fortawesome/free-regular-svg-icons": "^7.1.0",
        "@fortawesome/free-solid-svg-icons": "^7.1.0",
        "@fortawesome/vue-fontawesome": "^3.0.8",
        "@primeuix/themes": "^1.2.5",
        "@supabase/supabase-js": "^2.76.1",
        "@tailwindcss/vite": "^4.1.15",
        "dotenv": "^17.2.3",
        "electron-updater": "^6.6.2",
        "pinia": "^3.0.3",
        "primeicons": "^7.0.0",
        "primevue": "^4.4.1",
        "tailwindcss": "^4.1.15",
        "tailwindcss-primeui": "^0.6.1",
        "vue-data-ui": "^3.6.0",
        "vue-draggable-next": "^2.2.1",
        "vue-i18n": "^11.1.12",
        "vue-router": "^4.6.3"
    },
    "devDependencies": {
        "@electron-toolkit/eslint-config-prettier": "3.0.0",
        "@electron-toolkit/eslint-config-ts": "^3.1.0",
        "@electron-toolkit/tsconfig": "^2.0.0",
        "@primevue/auto-import-resolver": "^4.4.1",
        "@types/node": "^22.18.6",
        "@vitejs/plugin-vue": "^6.0.1",
        "electron": "^38.1.2",
        "electron-builder": "^25.1.8",
        "electron-devtools-installer": "^4.0.0",
        "electron-vite": "^4.0.1",
        "eslint": "^9.36.0",
        "eslint-plugin-vue": "^10.4.0",
        "prettier": "^3.6.2",
        "typescript": "^5.9.2",
        "unplugin-vue-components": "^30.0.0",
        "vite": "^7.1.6",
        "vue": "^3.5.21",
        "vue-eslint-parser": "^10.2.0",
        "vue-tsc": "^3.0.7"
    }
}
